        <script type="rocketlazyloadscript" data-rocket-type="text/javascript">

            function updateDatalayerAndRegister(doRegister) {

                const checkbox = document.getElementById('00N1U00000VWewP');
                if (checkbox.checked) {
                    checkbox.value = "1";
                } else {
                    checkbox.value = "0";
                }
                const analyticsActivities = JSON.parse(JSON.stringify(window.dataLayer));
                const analyticsEvents = analyticsActivities.filter(t => Object.keys(t).includes("event"));

                // Different Activities
                const clicks = analyticsEvents.filter(a => a.event === "gtm.click");
                const scrollDepths = analyticsEvents.filter(a => a.event === "gtm.scrollDepth");
                const videos = analyticsEvents.filter(a => a.event === "gtm.video");
                const timers = analyticsEvents.filter(a => a.event === "gtm.timer");

                // Max scroll depth
                let maxScrolled = 0;
                if (scrollDepths.length > 0) {
                    maxScrolled = scrollDepths.sort(
                        function (a, b) {
                            return parseInt(b['gtm.scrollThreshold']) - parseInt(a['gtm.scrollThreshold']);
                        }
                    )[0]['gtm.scrollThreshold'];
                }
                console.log("Max Scrolled: " + maxScrolled);
                document.getElementById("GA_Max_Scroll_Depth__c").value = maxScrolled;

                // Video Played or Not
                console.log("Video Played: " + (videos.length > 0));
                document.getElementById("GA_Video_Played__c").value = (videos.length > 0);

                if (videos.length > 0) {
                    const videoGroups = videos.reduce((acc, obj) => {
                        acc[obj['gtm.videoUrl']] = acc[obj['gtm.videoUrl']] || [];
                        acc[obj['gtm.videoUrl']].push(obj);
                        return acc;
                    }, {});

                    // Video Urls
                    console.log("Video Urls: " + Object.keys(videoGroups).join(', '));
                    document.getElementById("GA_Video_Links__c").value = Object.keys(videoGroups).join('\n ');


                    let totalElapsed = 0;
                    for (var key in videoGroups) {
                        if (videoGroups.hasOwnProperty(key)) {
                            let videoArray = videoGroups[key];
                            const maxElapsed = videoArray.sort(
                                function (a, b) {
                                    return parseInt(b['gtm.uniqueEventId']) - parseInt(a['gtm.uniqueEventId']);
                                }
                            )[0]['gtm.videoElapsedTime'];
                            //console.log("Video Elapsed: " + maxElapsed);
                            totalElapsed += maxElapsed;
                        }
                    }
                    // Total time spent on Video
                    console.log("Total Video Elapsed: " + totalElapsed);
                    document.getElementById("GA_Video_Time_Elapsed__c").value = totalElapsed;
                }

                // Storing user's device details
                const details = navigator.userAgent;

                const regexp = /android|iphone|kindle|ipad/i;
                const isMobileDevice = regexp.test(details);
                console.log("Is Mobile Device: " + isMobileDevice);
                document.getElementById("GA_User_Device__c").value = isMobileDevice ? 'Mobile' : 'Web';

                // Time spent on Page
                let totalTimeSpent = 0;
                if (timers.length > 0) {
                    maxTimeSpent = timers.sort(
                        function (a, b) {
                            return parseInt(b['gtm.timerEventNumber']) - parseInt(a['gtm.timerEventNumber']);
                        }
                    )[0]['gtm.timerElapsedTime'];
                    totalTimeSpent = Math.round(maxTimeSpent / 1000);
                }
                console.log("Total Time on Landing Page: " + totalTimeSpent);
                document.getElementById("GA_Time_On_Landing_Page__c").value = totalTimeSpent;

                //console.log("Analytics: " + JSON.stringify(analyticsEvents));
                //document.getElementById("Analytics_Data__c").value = JSON.stringify(analyticsEvents);

                // Lead Source
                const queryString = window.location.search;
                const urlParams = new URLSearchParams(queryString);
                let leadSource = 'Cadastro Blog';
                if (urlParams.has('utm_source')) {
                    const utm_source = urlParams.get('utm_source');
                    if (utm_source === 'angg') {
                        leadSource = 'Anúncio Google';
                    } else if (utm_source === 'anfb') {
                        leadSource = 'Anúncio Facebook';
                    } else if (utm_source === 'blog') {
                        //leadSource = 'Blog';
                    } else if (utm_source === 'email') {
                        leadSource = 'Fluxo Email';
                    } else if (utm_source === 'zap') {
                        leadSource = 'Fluxo Whatsapp';
                    } else if (utm_source === 'anex') {
                        leadSource = 'Mídia Externa';
                    } else if (utm_source === 'rsyt') {
                        leadSource = 'Youtube';
                    } else if (utm_source === 'rsitf') {
                        leadSource = 'Instagram Feed';
                    } else if (utm_source === 'rsitd') {
                        leadSource = 'Instagram Direct';
                    } else if (utm_source === 'rsfbf') {
                        leadSource = 'Facebook Feed';
                    } else if (utm_source === 'rsfbms') {
                        leadSource = 'Facebook Messenger';
                    } else if (utm_source === 'rslkd') {
                        leadSource = 'LinkedIn';
                    }
                }
                document.getElementById("lead_source").value = leadSource;

                // Creative Ad(Anuncio_Criativo)
                if (urlParams.has('utm_content')) {
                    document.getElementById("Anuncio_Criativo__c").value = urlParams.get('utm_content')
                }


                // Joint Announcement(Anuncio Conjunto)
                if (urlParams.has('utm_adset_name')) {
                    document.getElementById("Anuncio_Conjunto__c").value = urlParams.get('utm_adset_name')
                }

                // Campaign Announcement(Anuncio Campanha)
                if (urlParams.has('utm_campaign')) {
                    document.getElementById("Anuncio_Campanha__c").value = urlParams.get('utm_campaign')
                }

                // Update Cep removing '-'
                document.getElementById("00N4w00000HiyFC").value = document.getElementById("cep").value.replace(/-/g, '');

                const formatZipCode = (event) => {
                    const target = event.target;
                    const t = target.value.replace(/-/g, '');
                    const s = target.selectionStart;
                    const b = t.substr(0, 5);
                    const e = t.substr(5);
                    if (e.length) {
                        target.value = [b, e].join('-');
                        if (s <= 5) target.setSelectionRange(s, s);
                        if (s == 6) target.setSelectionRange(s + 1, s + 1);
                    } else target.value = b;
                };

                window.onload = function () {
                    const inputCep = document.getElementById('cep');
                    inputCep.addEventListener('keydown', enforceFormat);
                    inputCep.addEventListener('keyup', formatZipCode);
                };

                // Campaign Mapping
                const origin = window.location.origin;
                const urlArray = window.location.href.split('?');
                let baseUrl = urlArray[0];
                if (baseUrl.slice(-1) === '/') {
                    baseUrl = baseUrl.slice(0, -1);
                }

                //console.log("Base Url: " + baseUrl);	
                document.getElementById("Campaign_Url__c").value = baseUrl;
                //document.getElementById("retURL").value = origin + '/pt/obrigado/';
                document.getElementById("retURL").value = origin + '/pt/obrigado/';
                document.getElementById("Blog_Url__c").value = window.location.href.split('?')[0];
				document.getElementById("url").value = window.location.href.split('?')[0];
				

                if (doRegister) {
                    document.getElementById("register-button").click();
                    gtag("event", "generate_lead", {
                        currency: "BRL",
                        value: 1.0,
                        href: baseUrl
                    });
                } else {
                    const full_url = window.location.href;
                    const chat_url = full_url.split('?')[0];
                    embedded_svc.settings.extraPrechatFormDetails = [
                        {
                            "label": "Source URL",
                            "value": chat_url,
                            "displayToAgent": true,
                            "transcriptFields": ["visitor_url__c"]
                        },
                        {
                            "label": "GA Chatbot Used",
                            "value": true,
                            "displayToAgent": true,
                            "transcriptFields": ["GA_Chatbot_Used__c"]
                        },
                        {
                            "label": "GA Max Scroll Depth",
                            "value": document.getElementById("GA_Max_Scroll_Depth__c").value,
                            "displayToAgent": true,
                            "transcriptFields": ["GA_Max_Scroll_Depth__c"]
                        },
                        {
                            "label": "GA Time On Landing Page",
                            "value": document.getElementById("GA_Time_On_Landing_Page__c").value,
                            "displayToAgent": true,
                            "transcriptFields": ["GA_Time_On_Landing_Page__c"]
                        },
                        {
                            "label": "GA User Device",
                            "value": document.getElementById("GA_User_Device__c").value,
                            "displayToAgent": true,
                            "transcriptFields": ["GA_User_Device__c"]
                        },
                        {
                            "label": "GA Video Links",
                            "value": document.getElementById("GA_Video_Links__c").value,
                            "displayToAgent": true,
                            "transcriptFields": ["GA_Video_Links__c"]
                        },
                        {
                            "label": "GA Video Played",
                            "value": document.getElementById("GA_Video_Played__c").value,
                            "displayToAgent": true,
                            "transcriptFields": ["GA_Video_Played__c"]
                        },
                        {
                            "label": "GA Video Time Elapsed",
                            "value": document.getElementById("GA_Video_Time_Elapsed__c").value,
                            "displayToAgent": true,
                            "transcriptFields": ["GA_Video_Time_Elapsed__c"]
                        },
                        {
                            "label": "Anúncio Campanha",
                            "value": document.getElementById("Anuncio_Campanha__c").value,
                            "displayToAgent": true,
                            "transcriptFields": ["Anuncio_Campanha__c"]
                        },
                        {
                            "label": "Anúncio Conjunto",
                            "value": document.getElementById("Anuncio_Conjunto__c").value,
                            "displayToAgent": true,
                            "transcriptFields": ["Anuncio_Conjunto__c"]
                        },
                        {
                            "label": "Anúncio Criativo",
                            "value": document.getElementById("Anuncio_Criativo__c").value,
                            "displayToAgent": true,
                            "transcriptFields": ["Anuncio_Criativo__c"]
                        }

                    ];
                }
            }



            // Initialize iframe api for youtube video analytics
            var tag = document.createElement('script');
            tag.src = "https://www.youtube.com/iframe_api";
            var firstScriptTag = document.getElementsByTagName('script')[0];
            firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

        </script>
    </div>
</div></div><div id="block-10" class="cb-sidebar-widget codetipi-15zine-widget widget_block widget_text">
<p></p>
