		<script type="rocketlazyloadscript">
			function submitUserForm() {
				var response = grecaptcha.getResponse();
				if(response.length == 0) {
					document.getElementById('g-recaptcha-error').innerHTML = '<span style="margin: 10px; color:red; font-size: 16px; font-weight: 500; display: flex; justify-content: center;">Esse campo é requerido</span>';
					return false;
				}
				return true;
			}
			 
			function verifyCaptcha() {
				document.getElementById('g-recaptcha-error').innerHTML = '';
			}
		</script>

        <style></style>
		
